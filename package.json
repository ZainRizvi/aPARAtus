{
  "name": "para-manager",
  "version": "1.0.0",
  "description": "Obsidian plugin for managing PARA folders: create and archive Projects, Areas, Resources",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build": "node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "install:dev": "node scripts/install-dev.mjs"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "para",
    "archive",
    "projects",
    "creation",
    "areas",
    "resources"
  ],
  "author": "ZainRizvi.io",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.0.0",
    "obsidian": "^1.7.2",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
